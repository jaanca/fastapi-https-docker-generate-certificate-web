{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: HTTPS",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--host","0.0.0.0",
                "--port","443",
                "--ssl-keyfile","cert-example.com/example.com.key",
                "--ssl-certfile","cert-example.com/example.com.crt",
                "--reload"
            ],
            "jinja": true
        }
    ]
}